questao A

#include <stdio.h>

int main(){
    
    
    printf("**********         **          *               *     \n");
    printf("*        *       *    *       ***             * *    \n");
    printf("*        *      *      *     *****           *   *   \n");
    printf("*        *      *      *    ********        *     *  \n");
    printf("*        *      *      *   **********        *   *   \n");
    printf("*        *       *   *         *              * *    \n");
    printf("**********         *           *               *     \n");

    
}

questao C

include <stdio.h>

int main(){
int a,b,soma,sub,divi,multi;

printf ("informe o primeiro numero \n");
scanf("%d", &a);
printf("informe o segundo numero \n");
scanf("%d", &b);

soma= a+b;
sub=a-b;
divi=a/b;
multi=a*b;

printf("a soma e : %d \n", soma);
printf("a subtracao e : %d \n", sub);
printf("a divisao e: %d \n", divi);
printf("a multiplicacao e : %d \n", multi);
    
}

questao D

include <stdio.h>

int main(){
   float c,f;
   
   printf("qual e a temperatura em graus celsius: \n");
   scanf("%f", &c);
   
    f= (9*c+160)/5;
    
    printf("a transformacao de celsius para fahrenheit e : %.2f\n", f);
    
    
}

questao E

#include <stdio.h>

int main(){
   float c,f;
   
   printf("quala temperatura em graus fahrenheit \n");
   scanf("%f", &f);
   
    c =(f - 32) * 5 / 9;
    
    printf("a tranformacao de fahrenheit para celsius e : %.2f",c);
    
}

questao F 

#include <stdio.h>

#define PI 3.14159

int main(){
  float raio,altura,volume,volume_ml;
  
printf("informe o raio \n");
scanf("%f", &raio);

printf("informe a altura \n");
scanf("%f", &altura);

volume= PI*raio*altura;
volume_ml=volume * 1000;

printf("o volume e %.2f \n",volume);
printf("o volume em ML e %.2f \n", volume_ml);



}
