#include <stdio.h>

int main(){
    
  float n1,n2,n3,n4,media_aritmetica;

    printf( "informe a 1 nota: \n");
    scanf("%f", &n1);

    printf( "informe a 2 nota: \n");
    scanf("%f", &n2);

    printf( "informe a 3 nota: \n");
    scanf("%f", &n3);

    printf( "informe a 4 nota: \n");
    scanf("%f", &n4);    

    media_aritmetica = (n1+n2+n3+n4)/4;

    printf("a media e %.2f", media_aritmetica);



}

